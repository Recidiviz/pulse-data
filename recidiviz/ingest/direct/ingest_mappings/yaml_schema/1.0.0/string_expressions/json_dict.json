{
  "$schema": "http://json-schema.org/draft-07/schema",
  "description": "Function that serializes multiple (key, value) pairs into a single JSON string.",
  "type": "object",
  "required": ["$json_dict"],
  "additionalProperties": false,
  "properties": {
    "$json_dict": {
      "type": "object",
      "additionalProperties": false,
      "minProperties": 1,
      "patternProperties": {
        ".*": {
          "description": "A single entry in the serialized JSON dictionary. The key may have any name. The value expression for a given key will be evaluated and the result will be serialized into the JSON.",
          "$ref": "index.json"
        }
      }
    }
  },
  "examples": [
    {
      "$json_dict": {
        "my_json_key": "MY_COL",
        "my_json_key_2": "MY_COL_2"
      }
    },
    {
      "$json_dict": {
        "category_1": "ctgory_of_chrgs_1",
        "category_2": "ctgory_of_chrgs_2",
        "category_3": "ctgory_of_chrgs_3",
        "category_4": "ctgory_of_chrgs_4",
        "category_5": "ctgory_of_chrgs_5"
      }
    }
  ],
  "#contextualized_examples": [
    {
      "StateIncarcerationIncident": {
        "external_id": "misconduct_number",
        "incident_details":     {
          "$json_dict": {
            "category_1": "ctgory_of_chrgs_1",
            "category_2": "ctgory_of_chrgs_2",
            "category_3": "ctgory_of_chrgs_3",
            "category_4": "ctgory_of_chrgs_4",
            "category_5": "ctgory_of_chrgs_5"
          }
        }
      }
    }
  ]
}

